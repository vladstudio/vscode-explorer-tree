{
  "name": "vscode-explorer-tree",
  "displayName": "Explorer Tree Generator",
  "description": "Generate markdown tree structure from Explorer folders",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "explorerTree.generateTree",
        "title": "Generate tree"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "explorerTree.generateTree",
          "group": "7_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}