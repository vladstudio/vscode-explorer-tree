{
  "name": "vscode-explorer-tree",
  "displayName": "Explorer Tree Generator",
  "description": "Generate markdown tree structure from Explorer folders",
  "version": "0.0.2",
  "publisher": "vladstudio",
  "author": {
    "name": "vladstudio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vladstudio/vscode-explorer-tree.git"
  },
  "bugs": {
    "url": "https://github.com/vladstudio/vscode-explorer-tree/issues"
  },
  "homepage": "https://github.com/vladstudio/vscode-explorer-tree#readme",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "tree",
    "explorer",
    "markdown",
    "directory",
    "structure"
  ],
  "icon": "tree.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "explorerTree.generateTree",
        "title": "Generate tree"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "explorerTree.generateTree",
          "group": "7_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.9",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "micromatch": "^4.0.8"
  }
}
